setcps(120/60/4)

d1 $ degradeBy 0.1 $ swingBy (0.08) 8 $ sound "hh*16" 
   # shape 0.9 # coarse 0.8 # gain 0.7 # hpf 1000 # crush 3
   # legato (0.3 * perlin)

d2 $ swingBy (0.08) 8 $ fast 2 $ sound "<808bd:1(3,8) [mafz 808bd [~ 808bd?] [~ <808bd mafz>?]]>" 
   # speed "<[1 1.5 1] [1]>"
   # verb "<0 [0.8 0!3]>" 0.9 0.6 0.6


d3 $ n "<[e4,a4,c5][c4,g4,c5,e5][ds5,b5,fs6][e4,a4,c5][ds4,gs4,c5]>" # s "supersaw" # voice 0